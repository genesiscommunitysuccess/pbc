{
  "name": "@genesiscommunitysuccess/notify-pbc",
  "description": "Genesis Notify PBC",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=20.0.0"
  },
  "genxSeedConfig": {
    "persistAnswers": true,
    "processFiles": {
      "exclude": [
        ".git",
        ".genx",
        "node_modules",
        ".gradle",
        "gradle"
      ],
      "rename": true,
      "interpolateVariables": {
        "enabled": true,
        "include": [
          "Dockerfile",
          "product-info",
          ".kt",
          ".md"
        ]
      }
    }
  },
  "scripts": {
    "release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "semantic-release": "^21.0.7",
    "semver": "^7.5.4"
  }
}
