{
  "name": "@genesiscommunitysuccess/notify-pbc",
  "description": "Genesis Notify PBC",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=20.0.0"
  },
  "genxSeedConfig": {
    "exclude": [
      "README.md"
    ],
    "persistAnswers": true,
    "processFiles": {
      "exclude": [
        ".git",
        ".genx",
        "node_modules",
        ".gradle",
        "gradle"
      ],
      "rename": true,
      "interpolateVariables": {
        "enabled": true,
        "include": [
          "Dockerfile",
          "product-info",
          ".kt",
          ".md"
        ]
      }
    }
  },
  "dependencies": {
    "semver": "^7.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genesiscommunitysuccess/pbc.git",
    "directory": "notify/.genx"
  }
}
